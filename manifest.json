{
    "manifest_version": 3,
    "name": "Hide my Gmail",
    "version": "1.0",
    "description": "Hide sender's name and message",
    "permissions": ["activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  